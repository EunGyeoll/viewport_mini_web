<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.mycompany.viewport_mini_web.dao.CartDao">
<insert id="insertUserWithProduct" parameterType="Cart">
	<selectKey keyProperty="cid" order="BEFORE" resultType="int">
			select seq_cid.nextval from dual
		</selectKey>
	insert into cart(cid,cqty,cpid,cuid) values(#{cid},#{cqty},#{cpid},#{cuid})
</insert>
</mapper>